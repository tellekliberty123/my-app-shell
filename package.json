{
  "name": "my-app-shell",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:ts:node": "ts-node ./server.ts",
    "build": "ng build",
    "build:all": "npm run lint:all && npm run build:lib && npm run build:ssr",
    "build:app": "ng run my-app-shell:app-shell",
    "build:browser": "ng build --prod my-app-shell",
    "build:lib": "ng build lib-route-app",
    "build:server": "ng run my-app-shell:server --output-hashing none",
    "serve": "ng serve",
    "test": "ng test my-app-shell --source-map --watch false  --code-coverage",
    "test:all": "npm test && npm run test:lib",
    "test:lib": "ng test lib-route-app --source-map --watch false  --code-coverage",
    "lint": "ng lint my-app-shell",
    "lint:all": "npm run lint && npm run lint:lib",
    "lint:lib": "ng lint lib-route-app",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "prestart": "npm run build:ssr",
    "start": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && npm run build:server"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.2",
    "@angular/common": "~8.2.2",
    "@angular/compiler": "~8.2.2",
    "@angular/core": "~8.2.2",
    "@angular/forms": "~8.2.2",
    "@angular/platform-browser": "~8.2.2",
    "@angular/platform-browser-dynamic": "~8.2.2",
    "@angular/platform-server": "~8.2.2",
    "@angular/pwa": "^0.802.2",
    "@angular/router": "~8.2.2",
    "@angular/service-worker": "~8.2.2",
    "@nguniversal/common": "^8.1.1",
    "@nguniversal/express-engine": "^8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^8.1.1",
    "core-js": "^2.5.4",
    "express": "^4.17.1",
    "rxjs": "~6.5.2",
    "tslib": "^1.9.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.0",
    "@angular-devkit/build-ng-packagr": "^0.802.2",
    "@angular/cli": "~8.2.1",
    "@angular/compiler-cli": "~8.2.2",
    "@angular/language-service": "~8.2.2",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "reflect-metadata": "^0.1.13",
    "ts-loader": "^6.0.4",
    "ts-node": "^7.0.1",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "^3.5.3",
    "webpack-cli": "^3.3.12"
  }
}
